    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "permissions":[ "CAMERA", "WRITE_EXTERNAL_STORAGE", "CAMERA_ROLL" ],
      "package": "vendor.expresscab",
